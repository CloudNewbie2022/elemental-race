{
    "name": "elemental-proxy-server",
    "version": "1.0.0",
    "description": "Proxy server for Elemental Race leaderboard",
    "main": "server.js",
    "scripts": {
      "dev": "nodemon proxy/server.js",
      "start": "concurrently \"npm run start:frontend\" \"npm run start:proxy\"",
      "start:frontend": "live-server --port=5500 --host=127.0.0.1 ./",
      "start:proxy": "node proxy/server.js"
    },
    "dependencies": {
      "cors": "^2.8.5",
      "express": "^5.1.0",
      "node-fetch": "^3.3.2"
    },
    "devDependencies": {
      "nodemon": "^3.1.9"
    }
  }
  